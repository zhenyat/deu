  <br/>
  <h3 class="font-bold text-xl text-yellow-700"><%= NounExample.model_name.human(count: 2) %></h3>

  <table class="min-w-full table-auto border border-collapse border-slate-500">
    <thead class="text-xl text-gray-700 bg-green-200">    
    <tbody class='bg-green-100'>
      <% object.noun_examples.each do |example| %>
        <tr>
          <td><%= example.de %></td>
          <td><%= example.en %></td>
          <td><%= example.ru %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
